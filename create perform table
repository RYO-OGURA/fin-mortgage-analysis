    
CREATE TABLE perform_1999
(
	loan_seq_no char(12),
    m_period char(6),
    actual_upb decimal(14,2),
    dlq_status char(3),
    l_age int(3),
    rem_mth int(3),
    repur char(1),
    mdfied char(1),
    z_bal int(2),
    zerodate char(6),
    i_rate decimal(6,3),
    deferred_upb int(12),
    ddlpi char(6),
    mi_recover int(12),
    sales_proceeds varchar(14),
    non_mi_recoversies int(12),
    expenses int(12)
)DATA directory = 'H:/mysql';


/* perform data load */
LOAD DATA CONCURRENT LOCAL INFILE 'C:/rawdata/performQ11999.txt'
IGNORE
    INTO TABLE perform_1999
    CHARACTER SET default
    FIELDS
        TERMINATED BY '|'
    LINES
        STARTING BY ''
        TERMINATED BY '\n'
	(	loan_seq_no,
    m_period,
    actual_upb,
    dlq_status,
    l_age,
    rem_mth,
    repur,
    mdfied,
    z_bal,
    zerodate,
    i_rate,
    deferred_upb,
    ddlpi,
    mi_recover,
    sales_proceeds,
    non_mi_recoversies,
    expenses);
    
