    CREATE TABLE perform_2006
(
	loan_seq_no char(12),
    m_period char(6),
    actual_upb decimal(14,2),
    dlq_status char(3),
    l_age int(3),
    rem_mth int(3),
    repur char(1),
    mdfied char(1),
    z_bal int(2),
    zerodate char(6),
    i_rate decimal(6,3),
    deferred_upb int(12),
